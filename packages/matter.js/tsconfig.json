/* Dev configuration */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "esModuleInterop": true, // Required to remove warnings or jest test
    "composite": true,
    "module": "commonjs"
  },
  /* During dev, src are included, so they are also modified during refactorings. */
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
