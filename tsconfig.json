{
    "compilerOptions": {
        "declaration": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "strictNullChecks": true
    },
    "exclude": ["**/*.d.ts"]
}
