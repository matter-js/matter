    "dependencies": {
        "@project-chip/matter.js": "0.7.3-alpha.0-20231217-383ffe47",
        "@project-chip/matter.js-intermediate-models": "0.7.3-alpha.0-20231217-383ffe47"
    },
    "engines": {
        "node": ">=18.0.0"
    },
